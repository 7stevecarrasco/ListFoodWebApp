@model IEnumerable<FoodCategory>

<ul>
    @foreach (var item in Model)
    {
        <li class="@item.FoodCategoryID">
          @*  @if (item.Active)
            {
                <div class="side-menu-collapsible" style="font-size: 14px; font-weight:bold" onclick="ExpandCollapseMenu(this,'@item.FoodCategoryID')">
                    @if (item.Children.Any())
                    {
                        <i class="fa fa-caret-right"></i>
                    }
                    <a>@Html.DisplayFor(modelItem => item.Name)</a>
                </div>
            }
            else
            {
                <div class="side-menu-collapsible" style="display: none;">
                    @if (item.Children.Any())
                    {
                        <i class="fa fa-caret-right"></i>
                    }
                    <a>@Html.DisplayFor(modelItem => item.Name)</a>
                </div>
            }*@

            @if (item.Children.Any())
            {

                @await Html.PartialAsync("_TreeViewFoodCategory", item.Children)
            }
        </li>
    }
</ul>

